<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <EfcptConfigDbContextNameSpace>Models</EfcptConfigDbContextNameSpace>
    <EfcptConfigModelNamespace>Models</EfcptConfigModelNamespace>
    <EfcptLogVerbosity>detailed</EfcptLogVerbosity>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="10.0.0" />
    <PackageReference Include="JD.Efcpt.Build" Version="0.9.2" />
  </ItemGroup>

</Project>
