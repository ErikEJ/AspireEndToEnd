<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <EfcptConfigRootNamespace>DataAccess</EfcptConfigRootNamespace>
    <EfcptConfigDbContextName>DatabaseContext</EfcptConfigDbContextName>
    <EfcptConfigDbContextNameSpace>Models</EfcptConfigDbContextNameSpace>
    <EfcptConfigModelNamespace>Models</EfcptConfigModelNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="10.0.0" />
    <PackageReference Include="JD.Efcpt.Build" Version="0.8.0" />
  </ItemGroup>

</Project>
